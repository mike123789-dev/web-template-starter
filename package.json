{
  "name": "web-template-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:open": "next dev --hostname 127.0.0.1 --port 3000 & sleep 2; open http://127.0.0.1:3000",
    "dev:stop": "lsof -tiTCP:3000 -sTCP:LISTEN | xargs -r kill",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "agent-browser": "^0.9.0",
    "autoprefixer": "latest",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "postcss": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  }
}
